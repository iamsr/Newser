<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shubhamr.newser">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_newser_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_newser_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Views.Activity.MainActivity"
            android:icon="@drawable/ic_newser">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Views.Activity.SourceActivity" />
        <activity android:name=".Views.Activity.NewsDisplayActivity"></activity>
    </application>

</manifest>